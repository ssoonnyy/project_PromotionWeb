ader {\r\n  display: flex;\r\n  >ul{\r\n    display: flex;\r\n    align-items: end;\r\n    &.header_lnb { \r\n      font-family: 'Oswald', sans-serif; \r\n \r\n    }\r\n    &.header_gnb {\r\n      li {\r\n        a{\r\n          img {\r\n            width: 1.5rem; height: 1.5rem;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n@media all and (min-width : 320px) and (max-width : 800px) {\r\n  .header{\r\n    height: 55px;\r\n    position: relative;\r\n    padding-top: 20px;\r\n    padding-bottom: 5px;\r\n    &::before{\r\n      content: ' ';\r\n      width: 100%; height: 20px;\r\n      background-color: #ccc;\r\n      position: absolute;\r\n      top: 0;\r\n    }\r\n    .header_lnb {\r\n      width: 33%;\r\n      order: 1;\r\n      justify-content: start;\r\n      position: relative;\r\n      >ul{\r\n        &:nth-child(1){\r\n          position: relative;\r\n          bottom: 0;\r\n          width: 2.03125rem; height: 1.7875rem;\r\n          display: inline-block;\r\n          transition: all .4s;\r\n          box-sizing: border-box;\r\n          margin: 0 0 3px 5px;\r\n          > span {\r\n            position: absolute;\r\n            left: 0;\r\n            width: 100%;\r\n            height: 3px;\r\n            background-color: black;\r\n            border-radius: 4px;\r\n            display: inline-block;\r\n            transition: all .4s;\r\n            box-sizing: border-box;\r\n            &:nth-child(1){top: 0;}\r\n            &:nth-child(1){top: 14px;}\r\n            &:nth-child(3){bottom: 0;}\r\n          }\r\n          &.active > span{\r\n            &:nth-child(1){transform: translateY(0px) rotate(-45deg);}\r\n            &:nth-child(2){opacity: 0;}\r\n            &:nth-child(3){transform: translateY(-12px) rotate(45deg);}  \r\n          }\r\n        }\r\n        &:nth-child(2){\r\n          display: none;\r\n          position: relative;\r\n          width: 100vw;\r\n          background-color: #fff;\r\n          transition: all 0.2s;\r\n          &.openLnb{\r\n            position: absolute;\r\n            bottom: calc(-167px - 70px - 7px - 5px);\r\n            display: flex;\r\n            flex-direction: column;\r\n            >li {\r\n              width: 100%;\r\n              padding: 5px 0;\r\n              text-align: center;\r\n              border-top: 1px solid #eee;\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n    .header_logo{\r\n      order: 2;\r\n      width: 33%;\r\n      justify-content: center;\r\n      >a {\r\n        width: 110px;\r\n      }\r\n    }\r\n    .header_gnb {\r\n      order: 3;\r\n      width: 33%;\r\n      justify-content: end;\r\n    }\r\n  }\r\n  .visual_main {\r\n    position: static;\r\n  }\r\n  \r\n} \r\n@media all and (min-width : 801px){\r\n  .header{\r\n    position: sticky;\r\n    top: 0;\r\n    z-index: 9999999999999999999;\r\n    box-sizing: border-box;\r\n    justify-content: space-between;\r\n    .header_logo {\r\n      max-width: 190px;\r\n      margin-left: 20px;\r\n      margin-top: 10px;\r\n    }\r\n    .header_lnb{\r\n      width: 100%;\r\n      >ul:nth-child(2){\r\n        width: 100%;\r\n        display: flex;\r\n        justify-content: space-around;\r\n        align-items: end;\r\n        >li {\r\n          >a{\r\n            color: #000;\r\n            display:inline-block; \r\n            margin:0;\r\n            text-transform:uppercase; \r\n            &:after{\r\n              display: block;\r\n              content: '';\r\n              border-bottom: solid 1px #ea2129;  \r\n              transform: scaleX(0);  \r\n              transition: transform 250ms ease-in-out;\r\n            }\r\n            &:hover:after { transform: scaleX(1); }\r\n          }\r\n        }\r\n      }\r\n    }\r\n    .header_gnb{\r\n      justify-content: space-evenly;\r\n      width: 12.5rem;\r\n    }\r\n  }\r\n  .visual_main {\r\n    position: absolute;\r\n    top: 0;\r\n  }\r\n}\r\n@media all and (min-width : 1024px ){\r\n  .header{\r\n    padding: 2.8125rem 10rem 0 ;\r\n    .header_logo {min-width: 190px;}\r\n  }\r\n}\r\n\r\n/*main*/\r\n\r\n  .visual_main{\r\n    width: 100%; height: 58.5625rem;\r\n    overflow: hidden;\r\n    >video {\r\n      width: 100%;\r\n    }\r\n  }\r\n"},"lineCount":null}},"error":null,"hash":"e2a151b5122b9c86c0138d0a09684fe7","cacheData":{"env":{}}}